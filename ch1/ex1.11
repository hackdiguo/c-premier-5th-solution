#include <iostream>
#include <cmath>
using namespace std;

int main()
{
  int v1, v2;
  std::cout << "please enter two integers "<<std::endl;
  std::cin >> v1 >> v2;
  int i = (v1 + v2 - abs(v1 -v2))/2;
  while( i <= (v1 + v2 + abs(v1 -v2))/2){
    std::cout << i << std::endl;
    ++i;
  }
  return 0;
}
